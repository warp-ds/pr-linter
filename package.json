  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@vercel/ncc": "^0.36.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0"
  }
}
